@{
    ViewData["Title"] = "Users Page";
    ViewBag.Current = "Users";
}
@model IEnumerable<ZantosMvc.Models.UserViewModel>

<partial name="~/Views/Admin/_PartialAdminMenu.cshtml" />

<h2>Users</h2>
<table class="table">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Mail</th>
            <th>Roles</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Email</td>
                <td>@user.Role</td>
            </tr>

        }
    </tbody>
</table> 